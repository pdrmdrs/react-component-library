    "watch:html": "html-scripts --action=start --entry=./src/index.ts"
  },
  "bugs": {
    "url": "https://github.com/pdrmdrs/react-component-library/issues"
  },
  "dependencies": {
    "@rcl/contact-screen": "^1.3.1",
    "@rcl/home-screen": "^1.2.1"
  },
  "devDependencies": {
    "@rcl/html-scripts": "^1.1.0"
  }
