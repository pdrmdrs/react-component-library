{
  "name": "@rcl/html",
  "version": "1.1.1",
  "description": "Entrypoint for the HTML version of the package",
  "author": "Pedro Medeiros <pdr.mdrs@gmail.com>",
  "homepage": "https://github.com/pdrmdrs/react-component-library/tree/master/entrypoints/html#readme",
  "license": "MIT",
  "main": "html-dist/index.js",
  "directories": {
    "src": "src",
    "test": "__tests__"
  },
  "files": [
    "html-dist"
  ],
  "publishConfig": {
    "registry": "https://registry.yarnpkg.com"
  },
